<h2>JavaScript Array Sort</h2>

<p>Welcome to kiet college</p>

<button onclick="myFunction()">click here</button>

<p id="demo"></p>

<script>
var fruits = ["pineapple", "Orange", "grapes", "Mango"];
document.getElementById("demo").innerHTML = fruits;

function myFunction() {
  fruits.sort();
  document.getElementById("demo").innerHTML = fruits;
}
</script>

